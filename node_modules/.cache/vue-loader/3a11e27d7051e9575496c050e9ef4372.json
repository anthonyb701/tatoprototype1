\n            class=\"create__component date__component\"\r\n            label=\"Кінець операції\"\r\n            prepend-icon=\"access_time\"\r\n            readonly\r\n            v-bind=\"attrs\"\r\n            v-on=\"on\"\r\n          ></v-text-field>\r\n        </template>\r\n        <v-time-picker\r\n          v-if=\"menuEnd\"\r\n          format=\"24hr\"\r\n          v-model=\"timeEnd\"\r\n          :min=\"timeStart\"\r\n          full-width\r\n          @click:minute=\"$refs.menuEnd.save(timeEnd)\"\r\n        ></v-time-picker>\r\n      </v-menu>\r\n      </div>\r\n      \r\n      \r\n      \r\n      \r\n      \r\n      <div class=\"form-comp\">\r\n        <v-textarea \r\n          name=\"textarea\"\r\n          v-model=\"description\"\r\n          class=\"create__component\" label=\"Опис Операції\"\r\n          hint=\"Введіть текст\"\r\n          required\r\n        ></v-textarea>\r\n      </div>\r\n      <div class=\"form-comp\">\r\n        <v-textarea\r\n          name=\"textarea\"\r\n          v-model=\"complexity\"\r\n          class=\"create__component\" label=\"Ускладнення\"\r\n          hint=\"Введіть текст\"\r\n          required\r\n        ></v-textarea>\r\n      </div>\r\n      <div class=\"form-comp\">\r\n        <v-textarea\r\n          name=\"textarea\"\r\n          v-model=\"gisto\"\r\n          class=\"create__component\" label=\"Гістологічне Заключення\"\r\n          hint=\"Введіть текст\"\r\n          required\r\n        ></v-textarea>\r\n      </div>\r\n      </div>\r\n      <div class=\"section__action\">\r\n        <div>\r\n          <div class=\"section__confirm\">\r\n          <v-checkbox\r\n          v-model=\"checkbox\"\r\n          value=\"1\"\r\n          label=\"Підтвердити\"\r\n          type=\"checkbox\"\r\n          required\r\n          ></v-checkbox>\r\n        </div>\r\n        <div class=\"section__buttons\">\r\n          <v-btn @click=\"clear\" class=\"pa-2 mr-4 white--text red darken-2 btn-width1\">Стерти <v-icon class=\"m